tion: row;\n    justify-content: space-between;\n    width: 210px;\n\n    span {\n      margin-right: 5px;\n    }\n\n    p {\n      max-width: 180px;\n    }\n  }\n\n\n`;\n\nexport default function CommentForm({ selectedImg }) {\n  const dispatch = useDispatch();\n  const [edit, setEdit] = useState(\"\");\n  const [like, setLike] = useState(false);\n  const loggedInUser = useSelector((store) => store.session.user);\n  let likeButtonText = <i className=\"fa fa-heart like-button\" aria-hidden=\"true\"></i>;\n\nconst currentComments = [selectedImg]\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n    //  window.location.href = '/pic';\n\n    const payload = {\n      photoId: selectedImg.id,\n      userId: loggedInUser.id,\n      edit,\n    };\n    console.log(payload);\n\n    dispatch(createComment(payload));\n    // history.push(`/gallery`);\n  };\n\n  const onLike = (e) => {\n    e.preventDefault();\n    setLike(!like)\n\n     const payload = {\n    photoId: selectedImg.id,\n    userId: loggedInUser.id,\n    like\n  };\n\n    console.log(selectedImg)\n    dispatch(likePhoto(payload));\n    \n  }\n  \n \n\n  useEffect(async () => {\n    // Request to the server.\n    // const response = await fetch(\"/api/bands\");\n    // setBands(response.data.bands);\n    const setLikedImg = () => selectedImg.like = like\n    setLikedImg()\n    \n  }, [like]);\n\n\n\n   if (selectedImg.like == true) {\n       likeButtonText = <i className=\"fa fa-heart like-button-liked\" aria-hidden=\"true\"></i>;\n    }\n\n\n  const Comment = ({theComment}) => {\n    return (\n      \n      <div>{theComment.Comments ? (\n          theComment.Comments.map((comment) => (\n            <CommentFormItem username={loggedInUser.username} userId={comment.userId} comment={comment.comment} />\n          ))\n        ) : (\n          <></>\n        )}</div>\n        \n    )\n  }\n\n  return (\n    <Comments>\n      <div>\n        <div className=\"comment-like\">\n          {/* <h2 className=\"h2-comment\"> {photo.title} </h2> */}\n          <h3>\n            Segen Shoots\n            {/* <Link\n            to={`/users/${photo.owner_id}/photos`}\n            className=\"comment-username\"\n          >\n            {photo.username}\n          </Link> */}\n          </h3>\n          <button onClick={onLike} value={like} className=\"like-button\">{likeButtonText}</button>\n          {/* {this.renderLikes()} */}\n          <div className=\"comment-form-container\">\n            <form className=\"comment-form\" onSubmit={onSubmit}>\n              <div className='comments-scroll'>\n              {/* {this.renderComments()} */}\n              {!currentComments && <h3>Loading...</h3>}\n              {currentComments &&\n                currentComments.map((comment) => {\n                  return <Comment theComment={comment} />;\n                })}\n                </div>\n              <div className=\"comment-div\">\n                <input\n                  type=\"text\"\n                  value={edit}\n                  onChange={(e) => setEdit(e.target.value)}\n                  // value={this.state.body}\n                  // onChange={this.handleInput(\"body\")}\n                  placeholder=\"Edit requests...\"\n                />\n                <br />\n              </div>\n              \n            </form>\n          </div>\n        </div>\n      </div>\n    </Comments>\n  );\n}\n","/Users/ahdariscott/Desktop/segen-site/frontend/src/components/index.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/components/ReviewTitle/index.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/components/Splash/index.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/containers/AboutMeContainer/index.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/components/AboutMe/AboutMeLeft.js",["253","254","255"],"import React, {useState} from 'react'\nimport { useDispatch, useSelector } from \"react-redux\";\nimport styled from 'styled-components'