userId}`, {\n    method: \"PUT\",\n    // headers: {\"Content-Type\": \"multipart/form-data\"},\n    body: formData,\n  });\n\n  dispatch(setProfile(res.data.profilePic));\n};\n\nexport const createProfile = (body) => async (dispatch) => {\n  const res = await fetch(`/api/profile/`, {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(body),\n  });\n  // const venues = await res.json();\n\n  if (res.ok) {\n    dispatch(setProfile(res.data.profile));\n  }\n};\n\nconst initialState = {\n  profile: null,\n  profilePic: null\n};\n\n//Reducer\nconst profileReducer = (state = initialState, action) => {\n  switch (action.type) {\n    case GET_PROFILE:\n      return {\n        ...state,\n        profile: action.profile,\n      };\n    case CREATE_PROFILE:\n      return { ...state, profilePic: action.payload };\n    case REMOVE_PROFILE:\n      return {\n        ...state,\n        profile: null\n      };\n      default:\n        return state;\n    }\n};\n\nexport default profileReducer;\n","/Users/ahdariscott/Desktop/segen-site/frontend/src/components/Message/Message.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/styles/Button.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/pages/Home/index.js",["217","218"],"import React from 'react'\nimport { CreatePost, Feed} from '../../containers/index'\nimport styled from 'styled-components';\nimport ScriptTag from 'react-script-tag'\n\nexport default function Home() {\n    return (\n        \n        <div className='Home'>\n            <CreatePost />\n            <Feed/>\n        </div>\n      \n    )\n}\n","/Users/ahdariscott/Desktop/segen-site/frontend/src/containers/index.js",[],"/Users/ahdariscott/Desktop/segen-site/frontend/src/containers/Post/index.js",["219","220","221","222","223","224"],"/Users/ahdariscott/Desktop/segen-site/frontend/src/containers/CreatePost/index.js",["225","226"],"/Users/ahdariscott/Desktop/segen-site/frontend/src/co